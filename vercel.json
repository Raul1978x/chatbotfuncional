{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "3000"
  },
  "buildCommand": "npm run build",
  "devCommand": "npm run start:dev",
  "outputDirectory": "dist",
  "framework": "nestjs"
}

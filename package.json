    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=20.18.0",
    "npm": ">=10.8.0"
  },
  "overrides": {
    "jimp": "^0.16.13"
  }
}
